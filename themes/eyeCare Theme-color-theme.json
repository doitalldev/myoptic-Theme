// This theme's colors are based on the original Monokai:
//   #1e1f1c (tab well, borders)
//   #272822 (editor background)
//   #414339 (selection)
//   #75715e (focus)
//   #f8f8f2 (editor foreground)
{
	"type": "dark",
	"colors": {
		"dropdown.background": "#ffffff",
		"list.activeSelectionBackground": "#fdfdfd",
		"quickInputList.focusBackground": "#414339",
		"dropdown.listBackground": "#1e1f1c",
		"list.inactiveSelectionBackground": "#e7e7e7",
		"list.hoverBackground": "#fdfdfd",
		"list.dropBackground": "#ffffff",
		"list.highlightForeground": "#f8f8f2",
		"button.background": "#fdfdfd",
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"selection.background": "#878b9180",
		"editor.selectionHighlightBackground": "#575b6180",
		"editor.selectionBackground": "#878b9180",
		"minimap.selectionHighlight": "#dfdfdf80",
		"editor.wordHighlightBackground": "#ededed",
		"editor.wordHighlightStrongBackground": "#6a6a9680",
		"editor.lineHighlightBackground": "#fcfcfc",
		"editorLineNumber.activeForeground": "#c2c2bf",
		"editorCursor.foreground": "#01a9b5",
		"editorWhitespace.foreground": "#464741",
		"editorIndentGuide.background": "#e1e1e1",
		"editorIndentGuide.activeBackground": "#F06099",
		"editorGroupHeader.tabsBackground": "#ffffff",
		"editorGroup.dropBackground": "#41433980",
		"tab.inactiveBackground": "#e2e2e2",
		"tab.activeBackground": "#ffffff",
		"tab.border": "#d6d6d6",
		"tab.inactiveForeground": "#000000", // needs to be bright so it's readable when another editor group is focused
		"tab.lastPinnedBorder": "#414339",
		"widget.shadow": "#00000098",
		"progressBar.background": "#F06099",
		"badge.background": "#ffffff",
		"badge.foreground": "#f8f8f2",
		"editorLineNumber.foreground": "#eaeae4",
		"panelTitle.activeForeground": "#818181",
		"panelTitle.activeBorder": "#ffffff",
		"panelTitle.inactiveForeground": "#75715E",
		"panel.border": "#d8d8d8",
		"panel.background": "#fcfcfc",
		"settings.focusedRowBackground": "#4143395A",
		"titleBar.activeBackground": "#ffffff",
		"statusBar.background": "#ececec",
		"statusBar.foreground": "#070707",
		"statusBar.noFolderBackground": "#ececec",
		"statusBar.debuggingBackground": "#F0E478",
		"statusBarItem.remoteBackground": "#D4172F",
		"ports.iconRunningProcessForeground": "#ccccc7",
		"activityBar.background": "#ffffff",
		"activityBar.foreground": "#000000",
		"sideBar.background": "#fcfcfc",
		"sideBarSectionHeader.background": "#ffffff",
		"menu.background": "#ffffff",
		"menu.foreground": "#000000",
		"pickerGroup.foreground": "#ffffff",
		"input.background": "#ffffff",
		"inputOption.activeBorder": "#75715E",
		"focusBorder": "#e5e5e5",
		"editorWidget.background": "#ffffff",
		"debugToolBar.background": "#ffffff",
		"diffEditor.insertedTextBackground": "#4b661680", // middle of #272822 and #00A8B5
		"diffEditor.removedTextBackground": "#90274A70", // middle of #272822 and #D4172F
		"inputValidation.errorBackground": "#90274A", // middle of #272822 and #D4172F
		"inputValidation.errorBorder": "#D4172F",
		"inputValidation.warningBackground": "#848528", // middle of #272822 and #e2e22e
		"inputValidation.warningBorder": "#e2e22e",
		"inputValidation.infoBackground": "#546190", // middle of #272822 and #819aff
		"inputValidation.infoBorder": "#819aff",
		"editorHoverWidget.background": "#f8f8f8",
		"editorHoverWidget.border": "#cdcdcd",
		"editorSuggestWidget.background": "#ffffff",
		"editorSuggestWidget.border": "#cdcdcd",
		"editorGroup.border": "#cdcdcd",
		"peekView.border": "#cdcdcd",
		"peekViewEditor.background": "#ffffff",
		"peekViewResult.background": "#ffffff",
		"peekViewTitle.background": "#ffffff",
		"peekViewResult.selectionBackground": "#ededed",
		"peekViewResult.matchHighlightBackground": "#ededed",
		"peekViewEditor.matchHighlightBackground": "#ededed",
		"terminal.ansiBlack": "#333333",
		"terminal.ansiRed": "#C4265E", // the bright color with ~75% transparent on the background
		"terminal.ansiGreen": "#86B42B",
		"terminal.ansiYellow": "#B3B42B",
		"terminal.ansiBlue": "#6A7EC8",
		"terminal.ansiMagenta": "#8C6BC8",
		"terminal.ansiCyan": "#56ADBC",
		"terminal.ansiWhite": "#e3e3dd",
		"terminal.ansiBrightBlack": "#666666",
		"terminal.ansiBrightRed": "#D4172F",
		"terminal.ansiBrightGreen": "#00A8B5",
		"terminal.ansiBrightYellow": "#e2e22e", // hue shifted #00A8B5
		"terminal.ansiBrightBlue": "#819aff", // hue shifted #AE81FF
		"terminal.ansiBrightMagenta": "#AE81FF",
		"terminal.ansiBrightCyan": "#432160",
		"terminal.ansiBrightWhite": "#f8f8f2"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#88846f"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#85aa00"
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#85aa00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#D4172F"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#D4172F"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D4172F"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F51900"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#D4172F"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#432160"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#432160"
			}
		},
		{
			"name": "Library class/type",
			"scope": "support.type, support.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#432160"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#F44747"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#CFCFC2"
			}
		},
		{
			"name": "diff.header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#D4172F"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#AE81FFA0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#D4172F"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Markup Styling",
			"scope": "markup.bold, markup.italic",
			"settings": {
				"foreground": "#432160"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#FD971F"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#00A8B5"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"foreground": "#00A8B5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Quote",
			"scope": "markup.quote.markdown",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75715E"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Link Title/Description",
			"scope": "string.other.link.title.markdown,string.other.link.description.markdown",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Markdown Underline Link/Image",
			"scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Markdown Emphasis",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markdown Punctuation Definition Link",
			"scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markdown List Punctuation",
			"scope": [
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#00A8B5"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#F06099"
			}
		}
	],
	"semanticHighlighting": true
}