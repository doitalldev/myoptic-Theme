{
  "name": "myoptic-Theme-color-theme",
  "displayName": "Myoptic Eye Care",
  "description": "A light theme for your eyes. Utilizing generated colors designed to help you read your code better with less eye strain.",
  "publisher": "doitalldev",
  "type": "light",
  "colors": {
      "editor.background": "#FAFAFA",
      "editor.foreground": "#333333",
      "editorLineNumber.foreground": "#d8d8d8",
      "editorLineNumber.activeForeground": "#7c7b7b",
      "editorCursor.foreground": "#CC3333",
      "editor.selectionBackground": "#ADD8E6",
      "editor.lineHighlightBackground": "#E0E0E0",
      "editorIndentGuide.background1": "#E0E0E0",
      "editorIndentGuide.activeBackground1": "#CCCCCC"
  },
  "tokenColors": [
      {
          "scope": [
              "comment",
              "punctuation.definition.comment",
              "string.comment"
          ],
          "settings": {
              "foreground": "#7D8B99"
          }
      },
      {
          "scope": [
              "constant",
              "variable.other.constant",
              "variable.language",
              "entity.other.attribute-name",
              // Liquid
              
          ],
          "settings": {
              "foreground": "#CC6600"
          }
      },
      {
          "scope": [
              "entity.name",
              "entity.name.function",
              "support.function",
              "support.constant.handlebars",
              "source.powershell variable.other.member",
              "variable.other.property.tsx"
              
          ],
          "settings": {
              "foreground": "#9933CC"
          }
      },
      {
          "scope": [
              "keyword",
              "storage.type",
              "storage.modifier",
              "entity.other.attribute-name.class",
              // Liquid
              "punctuation.output.liquid"
          ],
          "settings": {
              "foreground": "#CC3333"
          }
      },
      {
          "scope": [
              "string",
              "string punctuation.section.embedded source"
          ],
          "settings": {
              "foreground": "#009900"
          }
      },
      {
          "scope": [
              "support",
              "support.variable",
              "meta.import",
              "variable.other.readwrite",
              // Liquid
              "punctuation.definition.tag.begin.liquid",
              "punctuation.definition.tag.end.liquid"
          ],
          "settings": {
              "foreground": "#0066CC"
          }
      },
      {
          "scope": [
              "variable",
              "meta.definition.variable.name",
              "support.variable",
              "punctuation.definition.variable",
              "string.unquoted.js",

              // Liquid
              "support.class.object.liquid",
              
          ],
          "settings": {
              "foreground": "#333333"
          }
      },
      {
          "scope": [
              "entity.name.type",
              "entity.name.class",
              "support.type",
              "support.class"
          ],
          "settings": {
              "foreground": "#9933CC"
          }
      },
      {
          "scope": [
              "meta.function-call",
              "variable.function",
              "support.function",
              "keyword.other.special-method"
          ],
          "settings": {
              "foreground": "#9933CC"
          }
      },
      {
          "scope": [
              "invalid",
              "invalid.illegal"
          ],
          "settings": {
              "foreground": "#CC3333",
              "background": "#FAFAFA"
          }
      },
  ]
}
